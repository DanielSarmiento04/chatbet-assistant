{
  "/api/*": {
    "target": "http://localhost:8000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "info",
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/ws/*": {
    "target": "http://localhost:8000",
    "secure": false,
    "changeOrigin": true,
    "ws": true,
    "logLevel": "info"
  }
}
